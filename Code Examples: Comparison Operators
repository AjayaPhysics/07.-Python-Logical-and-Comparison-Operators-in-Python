# Comparison examples
a = 10
b = 20

# Equal to
print("a == b:", a == b)  # Output: False

# Not equal to
print("a != b:", a != b)  # Output: True

# Greater than
print("a > b:", a > b)    # Output: False

# Less than
print("a < b:", a < b)    # Output: True

# Greater than or equal to
print("a >= b:", a >= b)  # Output: False

# Less than or equal to
print("a <= b:", a <= b)  # Output: True
